{
	"name": "@titanium/webdialog",
	"version": "2.1.6",
	"description": "Axway Amplify modules  to use native SFSafariViewController (iOS) and Chrome Pages (Android) with Appcelerator Titanium apps",
	"main": "index.js",
	"homepage": "https://brenton.house/saying-goodbye-to-axway-amplify-titanium-31a44f3671de",
	"scripts": {
		"list": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
		"postinstall": "npx @titanium/tiapp-xml install",
		"postuninstall": "npx @titanium/tiapp-xml uninstall",
		"test": "npm install --ignore-scripts && npm run list",
		"prepublishOnly": "git pull && bump insane",
		"postpublish2": "npm deprecate ${npm_package_name}@${npm_package_version} \"Product retired.  No longer maintained.\""
	},
	"author": "Axway",
	"maintainers": [
		{
			"name": "Brenton House",
			"email": "brenton.house@gmail.com",
			"url": "https://brenton.house"
		}
	],
	"repository": {
		"type": "git",
		"url": "github:brentonhouse/titanium-webdialog"
	},
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/brentonhouse/titanium-webdialog/issues"
	},
	"keywords": [
		"titanium",
		"mobile",
		"alloy",
		"turbo",
		"axway",
		"appcelerator",
		"ios",
		"android",
		"iphone",
		"ipad",
		"webdialog",
		"safari",
		"titanium-native-module",
		"sdk",
		"plugin",
		"amplify",
		"api-builder",
		"appcelerator-titanium",
		"titanium-sdk",
		"titanium-mobile",
		"titanium-module",
		"axway-sdk",
		"axway-amplify",
		"brenton-house"
	],
	"titanium": {
		"type": "native-module",
		"platform": {
			"ios": {
				"moduleid": "ti.webdialog",
				"name": "titanium-web-dialog",
				"version": "2.0.0",
				"apiversion": 2,
				"architectures": [
					"armv7",
					"arm64",
					"i386",
					"x86_64"
				],
